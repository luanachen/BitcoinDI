  target 'BitcoinDITests' do
    inherit! :search_paths
    pod 'Swinject', '2.1.1'
    pod 'SwinjectAutoregistration', '2.1.1'
  end

  target 'BitcoinDIUITests' do
    inherit! :search_paths
  end

end
